{
  "routes": [
    { "src": "/robots.txt", "dest": "/robots.txt" },
    { "src": "/sitemap.xml", "dest": "/sitemap.xml" },
    { "src": "/logo.png", "dest": "/logo.png" },
    { "src": "/unickpdf.pdf", "dest": "/unickpdf.pdf" },
    { "src": "/videos/(.*)", "dest": "/videos/$1" },
    { "src": "/sobre", "dest": "/sobre.html" },
    { "src": "/como-funciona", "dest": "/como-funciona.html" },
    { "src": "/contato", "dest": "/contato.html" }
  ],
  "headers": [
    {
      "source": "/robots.txt",
      "headers": [
        { "key": "Content-Type", "value": "text/plain" },
        { "key": "Cache-Control", "value": "no-cache" }
      ]
    },
    {
      "source": "/sitemap.xml",
      "headers": [
        { "key": "Content-Type", "value": "application/xml" },
        { "key": "Cache-Control", "value": "no-cache" }
      ]
    }
  ]
}
